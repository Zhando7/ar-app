<a-assets>
    <video
        id="video1"
        src="/videos/video1.mp4"
        preload="auto"
        autoplay="false"
        response-type="arraybuffer"
        loop
        crossorigin
        webkit-playsinline
        playsinline
    ></video>
</a-assets>

<a-marker
    video1-handler
    type="pattern"
    preset="custom"
    url="/aframe/patterns/video1.patt"
    smooth="true"
    smoothCount="10"
    smoothTolerance="0.01"
    smoothThreshold="5"
    raycaster="objects: .clickable"
    emitevents="true"
    cursor="fuse: false; rayOrigin: mouse;"
>
    
    <a-video
        src="#video1"
        scale="1 1 1"
        position="0 0.1 0"
        rotation="-90 0 0"
        class="clickable"
        gesture-handler
    ></a-video>
</a-marker>